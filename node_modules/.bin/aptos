#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jjjf/Projects/petra-staking-app/node_modules/@aptos-labs/aptos-cli/dist/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules/@aptos-labs/aptos-cli/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules/@aptos-labs/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules:/Users/jjjf/Projects/node_modules:/Users/jjjf/node_modules:/Users/node_modules:/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jjjf/Projects/petra-staking-app/node_modules/@aptos-labs/aptos-cli/dist/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules/@aptos-labs/aptos-cli/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules/@aptos-labs/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules:/Users/jjjf/Projects/node_modules:/Users/jjjf/node_modules:/Users/node_modules:/node_modules:/Users/jjjf/Projects/petra-staking-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@aptos-labs/aptos-cli/dist/aptos.js" "$@"
else
  exec node  "$basedir/../@aptos-labs/aptos-cli/dist/aptos.js" "$@"
fi
