{
  "version": 2,
  "trailingSlash": false,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/dist/client/assets/:path*"
    },
    {
      "source": "/_vike/:path*",
      "destination": "/dist/client/_vike/:path*"
    },
    {
      "source": "/favicon.ico",
      "destination": "/dist/client/favicon.ico"
    },
    {
      "source": "/icon.svg",
      "destination": "/dist/client/icon.svg"
    },
    {
      "source": "/apple-touch-icon.png",
      "destination": "/dist/client/apple-touch-icon.png"
    },
    {
      "source": "/manifest.json",
      "destination": "/dist/client/manifest.json"
    },
    {
      "source": "/blog/_astro/:path*",
      "destination": "https://v0-aptos-staking.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://v0-aptos-staking.vercel.app/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/api/index"
    }
  ]
}
