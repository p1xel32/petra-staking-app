{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/search",
      "statusCode": 410,
      "destination": "/"
    },
    {
      "source": "/staking",
      "statusCode": 410,
      "destination": "/"
    },
    {
      "source": "/faucet",
      "statusCode": 410,
      "destination": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/blog/_astro/:path*",
      "destination": "https://v0-aptos-staking.vercel.app/_astro/:path*"
    },
    {
      "source": "/blog",
      "destination": "https://v0-aptos-staking.vercel.app"
    },
    {
      "source": "/blog/:path*",
      "destination": "https://v0-aptos-staking.vercel.app/:path*"
    }
  ]
}